  "scripts": {
    "start": "nodemon --watch \"./**/*.ts\" --exec \"ts-node\" ./src/index.ts",
    "build": "npx tsc",
    "start-prod": "npm run build && node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "test": "npm run clean && set DB_USES=test&& db-migrate reset --env test && db-migrate up --env test && npm run build && jasmine", 
    "clean": "rimraf dist/"
  },
  "license": "ISC",
